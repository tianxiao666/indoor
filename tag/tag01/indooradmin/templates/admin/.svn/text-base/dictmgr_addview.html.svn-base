<script language="javascript" type="text/javascript">
function confirmLogout()
{
	if(confirm('您确定要退出登录吗？'))
	{
		return true;
	}
	return false;
}

function checkForm()
{
	var check = /^[a-zA-Z_]+$/;
	if(!check.test($('#CODE_TYPE').val()))
	{
		alert('分类编码只能是字母和下划线组成！');
		$('#CODE_TYPE').focus();
		return false;
	}
    if ($('#CODE_NAME').val() == '')
	{
		alert('请填写分类名称！');
		$('#CODE_NAME').focus();
		return false;
	}
	if ($('#MAX_LENGTH').val() == '')
	{
		alert('请填写最大长度！');
		$('#MAX_LENGTH').focus();
		return false;
	}
	$('#CDAOSYS_TYPE_CODE').submit();
	
}
</script>
